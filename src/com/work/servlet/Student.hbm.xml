<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.jspdev.biyesheji">
	<class
		name="Student"
		table="student"
	>
		<meta attribute="sync-DAO">true</meta>
		<id
			name="id"
			type="string"
			column="number"
		>
			<generator class="assigned"/>
		</id>

		<property
			name="name"
			column="name"
			type="string"
			not-null="true"
			length="20"
		/>
		<property
			name="sex"
			column="sex"
			type="string"
			not-null="true"
			length="5"
		/>
		<property
			name="age"
			column="age"
			type="java.lang.Long"
			not-null="true"
			length="19"
		/>
		<property
			name="department"
			column="department"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="address"
			column="address"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="phone"
			column="phone"
			type="string"
			not-null="true"
			length="20"
		/>
		<property
			name="inTime"
			column="inTime"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="outTime"
			column="outTime"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="password"
			column="password"
			type="string"
			not-null="true"
			length="20"
		/>
		<property
			name="college"
			column="college"
			type="string"
			not-null="true"
			length="50"
		/>
	
		<set
			name="courses"
			table="grade"
			cascade="all"
		>
			<key column="snumber"/>
			<many-to-many column="cnumber" class="Course"/>
		</set>


	</class>	
</hibernate-mapping>
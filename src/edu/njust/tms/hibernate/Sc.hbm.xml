<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="edu/njust/tms/hibernate">
	<class
		name="Sc"
		table="sc"
	>
		<meta attribute="sync-DAO">true</meta>
		<id
			name="id"
			column="id"
			type="java.lang.Long"
			unsaved-value="null"
		>
			<generator class="increment"/>
		</id>

		<property
			name="sno"
			column="sno"
			type="string"
			not-null="true"
			length="20"
		/>
		<property
			name="cno"
			column="cno"
			type="string"
			not-null="true"
			length="20"
		/>
		<property
			name="name"
			column="name"
			type="string"
			not-null="true"
			length="20"
		/>
		<property
			name="credit"
			column="credit"
			type="java.lang.Long"
			not-null="true"
			length="19"
		/>
		<property
			name="teacher"
			column="teacher"
			type="string"
			not-null="true"
			length="20"
		/>
		<property
			name="time"
			column="time"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="address"
			column="address"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="type"
			column="type"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="selected"
			column="selected"
			type="string"
			not-null="true"
			length="20"
		/>


	</class>	
</hibernate-mapping>